/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=aa);
    Nand(a=a, b=aa, out=bb);
    Nand(a=aa, b=b, out=cc);
    Nand(a=bb, b=cc, out=out);
}
